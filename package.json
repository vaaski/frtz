{
  "name": "frtz",
  "description": "Access your FRITZ!Box configuration from the command line without using its web UI.",
  "version": "0.4.2",
  "author": "vaaski @vaaski",
  "bin": {
    "frtz": "./bin/run"
  },
  "bugs": "https://github.com/vaaski/frtz/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.0.1",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "chalk": "^4.1.0",
    "cli-table3": "^0.6.0",
    "cli-ux": "^5.4.6",
    "frtz-core": "^0.2.0",
    "fs-jetpack": "^2.4.0",
    "is-windows": "^1.0.2",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "husky": "^4.2.5",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/vaaski/frtz",
  "keywords": [
    "frtz",
    "Fritz!Box",
    "router",
    "config",
    "cli",
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "frtz",
    "topics": {
      "devices": {
        "description": "list and interact with devices"
      },
      "config": {
        "description": "list profiles and configure frtz"
      }
    },
    "@oclif/plugin-warn-if-update-available": {
      "timeoutInDays": 7,
      "message": "<%= config.name %> update available from <%= chalk.red(config.version) %> to <%= chalk.greenBright(latest) %>.\nrun <%= chalk.underline('npm i -g frtz@latest') %> to update."
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "vaaski/frtz",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node fixdocs.js"
    }
  }
}
